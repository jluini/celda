[gd_resource type="Resource" load_steps=2 format=2]

[ext_resource path="res://tools/grog/grog_script.gd" type="Script" id=1]

[resource]
resource_name = "main"
script = ExtResource( 1 )
code_or_filename = "
start:

	.load_room room2

	.wait 2

	.say \"A long long time ago...\"

	.wait 1

	.load_room room1

	.wait 2

	you.say \"Hi\"
	room1/bust.say \"Hi.\"

	.wait 0.7

	you.say \"How are you doing?\"
	room1/bust.say \"I'm just fine.\"

	.wait 1

	you.say \"Ok.\"

	.wait 2

	room1/bust.say \"Come here man.\"

	you.walk to=pos3

	you.say \"What?\"

	room1/bust.say \"Nothing.\"

	.wait 1

	.enable_input
"
